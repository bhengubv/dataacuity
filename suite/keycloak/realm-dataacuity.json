      "clientId": "dataacuity-portal",
      "name": "DataAcuity Portal",
      "description": "Main platform portal - public SPA client",
      "enabled": true,
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "protocol": "openid-connect",
      "rootUrl": "https://dataacuity.co.za",
      "baseUrl": "/",
      "redirectUris": [
        "https://dataacuity.co.za/*",
        "https://portal.dataacuity.co.za/*",
        "http://localhost:5006/*",
        "http://localhost:3000/*"
      ],
      "webOrigins": [
        "https://dataacuity.co.za",
        "https://portal.dataacuity.co.za",
        "http://localhost:5006",
        "http://localhost:3000"
